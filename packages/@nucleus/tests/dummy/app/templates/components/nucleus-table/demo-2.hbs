{{#docs-demo as |demo|}}
  {{#demo.example name="nucleus-table2.hbs"}}
    {{#ember-table as |t|}}
      {{#t.head columns=columns enableResize=false enableReorder=true as |h|}}
        {{#h.row as |r|}}
          {{#r.cell as |column|}}
            {{column.name}}
          {{/r.cell}}
        {{/h.row}}
      {{/t.head}}
      {{#t.body rows=rows as |b|}}
        {{#b.row as |r|}}
          {{#r.cell as |cellValue|}}
            {{cellValue}}
          {{/r.cell}}
        {{/b.row}}
      {{/t.body}}
    {{/ember-table}}
  {{/demo.example}}
  {{demo.snippet "nucleus-table2.hbs"}}
  {{demo.snippet "table-component2.js" label="component.js"}}
{{/docs-demo}}