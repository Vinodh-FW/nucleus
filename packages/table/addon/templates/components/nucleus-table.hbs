{{#ember-table class="nucleus-table" as |t|}}
  {{#t.head columns=columns as |h|}}
    {{#h.row as |r|}}
      {{r.cell}}
    {{/h.row}}
  {{/t.head}}

  {{#t.body rows=rows as |b|}}
    {{#b.row as |r|}}
      {{#r.cell as |cellValue|}}
        {{cellValue}}
      {{/r.cell}}
    {{/b.row}}
  {{/t.body}}
{{/ember-table}}